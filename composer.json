{
    "name": "amicus/filament-employee-management",
    "description": "This is my package filament-employee-management",
    "keywords": [
        "amicus",
        "laravel",
        "filament-employee-management"
    ],
    "homepage": "https://github.com/amicus/filament-employee-management",
    "support": {
        "issues": "https://github.com/amicus/filament-employee-management/issues",
        "source": "https://github.com/amicus/filament-employee-management"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tomislav Lazar",
            "email": "lazar.tomislav1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Amicus\\FilamentEmployeeManagement\\": "src/",
            "Amicus\\FilamentEmployeeManagement\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amicus\\FilamentEmployeeManagement\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amicus\\FilamentEmployeeManagement\\FilamentEmployeeManagementServiceProvider"
            ],
            "aliases": {
                "FilamentEmployeeManagement": "Amicus\\FilamentEmployeeManagement\\Facades\\FilamentEmployeeManagement"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
